{
  "name": "@hikers-book/trips-api",
  "version": "0.8.0",
  "description": "Hiker's Book Trips API",
  "author": "radoslav.irha@gmail.com",
  "license": "MIT",
  "scripts": {
    "build": "pnpm run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "pnpm run copy:views && pnpm run barrels && NODE_ENV=localhost tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "pnpm run copy:views && cross-env NODE_ENV=production node dist/index.js",
    "format": "eslint --ext .ts --fix src",
    "lint": "eslint --ext .ts src",
    "test": "pnpm run config:test && cross-env NODE_ENV=test jest",
    "test:watch": "pnpm run test --watchAll",
    "config:localhost": "hb-cli config node_modules/@hikers-book/config/config/api.trips.json config/localhost.json -f json || true",
    "config:test": "hb-cli config node_modules/@hikers-book/config/config/api.trips.template.json config/test.json -f json || true",
    "config:env": "hb-cli config node_modules/@hikers-book/config/environment-variables/api.trips.json config/custom-environment-variables.json -f json || true",
    "config:clear": "find ./config -name '*.json' -type f -delete",
    "config:api": "pnpm run config:env && pnpm run config:localhost",
    "postinstall": "pnpm run config:api && pnpm run copy:views",
    "cleanup": "rm -rf coverage dist",
    "cleanup:modules": "rm -rf node_modules",
    "copy:views": "hb-cli copy-views"
  },
  "dependencies": {
    "@hikers-book/tsed-common": "workspace:*",
    "@tsed/ajv": "^7.43.1",
    "@tsed/common": "^7.43.1",
    "@tsed/core": "^7.43.1",
    "@tsed/di": "^7.43.1",
    "@tsed/engines": "^7.43.1",
    "@tsed/exceptions": "^7.43.1",
    "@tsed/ioredis": "^7.43.1",
    "@tsed/json-mapper": "^7.43.1",
    "@tsed/logger": "^6.6.3",
    "@tsed/logger-file": "^6.6.3",
    "@tsed/mongoose": "^7.43.1",
    "@tsed/platform-cache": "^7.43.1",
    "@tsed/platform-exceptions": "^7.43.1",
    "@tsed/platform-express": "^7.43.1",
    "@tsed/platform-log-middleware": "^7.43.1",
    "@tsed/platform-middlewares": "^7.43.1",
    "@tsed/platform-params": "^7.43.1",
    "@tsed/platform-response-filter": "^7.43.1",
    "@tsed/platform-views": "^7.43.1",
    "@tsed/schema": "^7.43.1",
    "@tsed/swagger": "^7.43.1",
    "ajv": "^8.12.0",
    "barrelsby": "^2.8.1",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^4.0.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "method-override": "^3.0.0",
    "mongoose": "^8.0.0"
  },
  "devDependencies": {
    "@hikers-book/cli": "workspace:*",
    "@hikers-book/config": "workspace:*",
    "@hikers-book/eslint-config": "workspace:*",
    "@hikers-book/jest": "workspace:*",
    "@tsed/cli-plugin-eslint": "4.6.4",
    "@tsed/cli-plugin-jest": "4.6.4",
    "@tsed/cli-plugin-mongoose": "4.6.4",
    "@tsed/testing-mongoose": "^7.43.1",
    "@types/body-parser": "^1.19.4",
    "@types/compression": "^1.7.4",
    "@types/config": "^3.3.2",
    "@types/cookie-parser": "^1.4.5",
    "@types/cookie-session": "^2.0.46",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/method-override": "^0.0.34",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.10",
    "@types/supertest": "^2.0.15",
    "@types/validator": "^13.11.5",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "tsed": {
    "convention": "conv_default",
    "architecture": "arc_default",
    "packageManager": "pnpm"
  },
  "engines": {
    "node": ">= 18.0.0",
    "pnpm": ">= 8.7.0"
  },
  "os": [
    "darwin",
    "linux"
  ]
}