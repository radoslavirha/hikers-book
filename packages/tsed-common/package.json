{
  "name": "@hikers-book/tsed-common",
  "version": "0.7.0",
  "description": "Hiker's Book Ts.ED Common utils",
  "exports": {
    "./exceptions": "./dist/exceptions/index.js",
    "./swagger": "./dist/swagger/index.js"
  },
  "scripts": {
    "build": "pnpm run barrels && npx tsc",
    "barrels": "barrelsby --config .barrelsby.json",
    "format": "eslint --ext .ts --fix src",
    "lint": "eslint --ext .ts src",
    "test": "cross-env NODE_ENV=test jest",
    "postinstall": "pnpm build || true",
    "cleanup": "rm -rf coverage dist"
  },
  "dependencies": {
    "@tsed/exceptions": "^7.36.8",
    "@tsed/openspec": "^7.36.8",
    "@tsed/swagger": "^7.36.8",
    "barrelsby": "^2.8.1",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@hikers-book/eslint-config": "workspace:*",
    "@hikers-book/jest": "workspace:*",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">= 18.0.0",
    "pnpm": ">= 8.7.0"
  },
  "os": [
    "darwin",
    "linux"
  ]
}
