{
  "name": "@hikers-book/types",
  "version": "0.7.0",
  "description": "Hiker's Book Ts.ED Types",
  "exports": {
    ".": "./dist/types/index.js",
    "./enums": "./dist/enums/index.js"
  },
  "scripts": {
    "build": "pnpm run barrels && npx tsc",
    "barrels": "barrelsby --config .barrelsby.json",
    "format": "eslint --ext .ts --fix src",
    "lint": "eslint --ext .ts src",
    "postinstall": "pnpm build || true",
    "cleanup": "rm -rf dist",
    "cleanup:modules": "rm -rf node_modules"
  },
  "dependencies": {
    "barrelsby": "^2.8.1"
  },
  "devDependencies": {
    "@hikers-book/eslint-config": "workspace:^",
    "@types/node": "^20.8.10",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">= 18.0.0",
    "pnpm": ">= 8.7.0"
  },
  "os": [
    "darwin",
    "linux"
  ]
}
