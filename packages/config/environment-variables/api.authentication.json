      "host": "HB_REDIS_DEFAULT_HOST",
      "port": "HB_REDIS_DEFAULT_PORT",
      "username": "HB_REDIS_DEFAULT_USERNAME",
      "password": "HB_REDIS_DEFAULT_PASSWORD"
    }
  },
  "session": {
