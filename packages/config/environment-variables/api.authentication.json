{
  "frontend": {
    "url": "HB_FRONTEND_URL"
  },
  "auth": {
    "facebook": {
      "clientID": "HB_AUTH_FACEBOOK_CLIENT_ID",
      "clientSecret": "HB_AUTH_FACEBOOK_CLIENT_SECRET",
      "callbackURL": "HB_AUTH_FACEBOOK_CALLBACK_URL"
    },
    "github": {
      "clientID": "HB_AUTH_GITHUB_CLIENT_ID",
      "clientSecret": "HB_AUTH_GITHUB_CLIENT_SECRET",
      "callbackURL": "HB_AUTH_GITHUB_CALLBACK_URL"
    },
    "google": {
      "clientID": "HB_AUTH_GOOGLE_CLIENT_ID",
      "clientSecret": "HB_AUTH_GOOGLE_CLIENT_SECRET",
      "callbackURL": "HB_AUTH_GOOGLE_CALLBACK_URL"
    }
  },
  "jwt": {
    "expiresIn": "HB_JWT_EXPIRES_IN",
    "expiresInRefresh": "HB_JWT_EXPIRES_IN_REFRESH"
  },
