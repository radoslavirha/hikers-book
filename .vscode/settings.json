{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "conventionalCommits.scopes": [
    "api",
    "k8s",
    "package",
    "ui"
  ],
  "search.exclude": {
    "**/coverage": true,
    "**/dist": true,
    "pnpm-lock.yaml": true,
    "ui/hikers-book/.angular": true
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "html",
    "scss"
  ]
}