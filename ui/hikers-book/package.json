{
  "name": "@hikers-book/hikers-book-ui",
  "version": "0.9.0",
  "description": "Hiker's Book UI",
  "scripts": {
    "start": "pnpm run generate && ng serve",
    "build": "pnpm run generate && ng build --localize",
    "watch": "pnpm run generate && ng build --watch --configuration development",
    "test": "pnpm run generate && ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "format": "eslint --fix src",
    "lint": "eslint src",
    "config": "hb-cli config node_modules/@hikers-book/config/config/ui.hikers-book.json src/assets/config/config.json -f json || true",
    "config:clear": "find ./src/assets/config -name '*.json' -type f -delete",
    "postinstall": "pnpm run config",
    "cleanup": "rm -rf coverage dist",
    "cleanup:modules": "rm -rf node_modules",
    "generate": "pnpm run generate:graphql && pnpm run generate:api && pnpm run generate:i18n",
    "generate:i18n": "ng extract-i18n",
    "generate:graphql": "graphql-codegen && pnpm run format",
    "generate:api": "pnpm run generate:api:authentication:auth && pnpm run generate:api:authentication:v1 && pnpm run generate:api:stages:v1 && pnpm run generate:api:trips:v1",
    "generate:api:authentication:auth": "ng-openapi-gen -c api.authentication.auth.json && pnpm run format",
    "generate:api:authentication:v1": "ng-openapi-gen -c api.authentication.v1.json && pnpm run format",
    "generate:api:stages:v1": "ng-openapi-gen -c api.stages.v1.json && pnpm run format",
    "generate:api:trips:v1": "ng-openapi-gen -c api.trips.v1.json && pnpm run format"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "16.2.2",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "16.2.2",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@apollo/client": "^3.0.0",
    "apollo-angular": "5.0.1",
    "graphql": "^16",
    "ng-openapi-gen": "^0.51.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular-eslint/builder": "16.1.1",
    "@angular-eslint/eslint-plugin": "16.1.1",
    "@angular-eslint/eslint-plugin-template": "16.1.1",
    "@angular-eslint/schematics": "16.1.1",
    "@angular-eslint/template-parser": "16.1.1",
    "@angular/cli": "~16.2.1",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/localize": "16.2.7",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-apollo-angular": "3.5.6",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-urql": "^3.7.3",
    "@hikers-book/cli": "workspace:*",
    "@hikers-book/config": "workspace:*",
    "@hikers-book/eslint-config": "workspace:*",
    "@hikers-book/jest": "workspace:*",
    "@types/jasmine": "~4.3.0",
    "@types/karma": "^6.3.8",
    "@types/karma-coverage": "^2.0.3",
    "@types/karma-jasmine": "^4.0.5",
    "@types/karma-jasmine-html-reporter": "^1.7.3",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-extract-i18n-merge": "2.7.1",
    "ng-mocks": "^14.11.0",
    "postcss": "^8.4.29",
    "prettier": "^3.1.1",
    "typescript": "~5.1.6"
  },
  "engines": {
    "node": ">= 18.0.0",
    "pnpm": ">= 8.7.0"
  },
  "os": [
    "darwin",
    "linux"
  ]
}